  <style name="Xyzu.View.LibraryItem.Header.CheckBox" parent="Xyzu.AppCompat.CheckBox">
    <item name="android:backgroundTint">@color/ColorSurface</item>
    <item name="android:buttonTint">@color/ColorOnSurface</item>
    <item name="android:drawableTint">@color/ColorOnSurface</item>
