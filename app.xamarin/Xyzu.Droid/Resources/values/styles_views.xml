    <item name="android:paddingVertical">@dimen/dp8</item>
    <item name="android:paddingHorizontal">@dimen/dp16</item>
    <item name="android:singleLine">true</item>
  </style>
  <style name="Xyzu.View.LibraryItem.Header.Text.Title" parent="Xyzu.View.Library.Text.Title">
    <item name="android:background">@drawable/xyzu_view_libraryheader_text_background</item>
    <item name="android:backgroundTint">@color/ColorSurface</item>
    <item name="android:ellipsize">marquee</item>
