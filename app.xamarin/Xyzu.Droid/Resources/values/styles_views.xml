  <!-- Xyzu.View.NowPlayling -->
  <style name="Xyzu.View.NowPlayling" parent="Xyzu.View" />
  <style name="Xyzu.View.NowPlayling.Container" parent="Xyzu">
    <item name="android:background">@color/ColorSurface</item>
    <item name="android:backgroundTint">@color/ColorSurface</item>
    <item name="android:clickable">false</item>
    <item name="android:outlineProvider">background</item>
    <item name="android:outlineAmbientShadowColor">@color/ColorSurface</item>
    <item name="android:outlineSpotShadowColor">@color/ColorSurface</item>
    
    <item name="app:cardElevation">@dimen/dp16</item>
    <item name="app:cardBackgroundColor">@color/ColorSurface</item>
    <item name="app:cardCornerRadius">@dimen/dp16</item>
    <item name="app:cardPreventCornerOverlap">true</item>
    <item name="app:cardUseCompatPadding">false</item>
    <item name="app:contentPadding">@dimen/dp0</item>
  </style>
  <style name="Xyzu.View.NowPlayling.Image" parent="Xyzu.AppCompat.Image">
    <item name="android:elevation">@dimen/dp16</item>
    <item name="android:gravity">center</item>
    <item name="android:layout_margin">@dimen/dp32</item>
    <item name="android:outlineProvider">background</item>
    <item name="android:outlineAmbientShadowColor">@color/ColorSurface</item>
    <item name="android:outlineSpotShadowColor">@color/ColorSurface</item>
  </style>
  <style name="Xyzu.View.NowPlayling.Text" parent="Xyzu.AppCompat.Text">
    <item name="android:clipToPadding">false</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:padding">@dimen/dp4</item>
    <item name="android:shadowColor">@color/ColorSurface</item>
    <item name="android:shadowDx">0</item>
    <item name="android:shadowDy">0</item>
    <item name="android:shadowRadius">8</item>
    <item name="android:singleLine">true</item>
    <item name="android:textAlignment">viewStart</item>
    <item name="android:textColor">@color/ColorOnSurface</item>
    <item name="android:textSize">@dimen/abc_text_size_subhead_material</item>
  </style>
  <style name="Xyzu.View.NowPlayling.Button" parent="Xyzu.AppCompat.ImageButton">
    <item name="android:background">@drawable/xyzu_view_nowplayling_button_background</item>
  </style>
  <style name="Xyzu.View.NowPlayling.Button.Menu" parent="Xyzu.View.NowPlayling.Button">
    <item name="android:padding">@dimen/dp8</item>
    <item name="android:layout_width">@dimen/dp48</item>
    <item name="android:layout_height">@dimen/dp48</item>
  </style>
  <style name="Xyzu.View.NowPlayling.Button.Player" parent="Xyzu.View.NowPlayling.Button">
    <item name="android:padding">@dimen/dp16</item>
    <item name="android:layout_width">@dimen/dp80</item>
    <item name="android:layout_height">@dimen/dp80</item>
  </style>
