    <item name="android:background">@drawable/icon_xyzu</item>
    <item name="android:backgroundTint">@color/ColorSurfaceVarient</item>
  </style>
  <style name="Xyzu.View.LibraryItem.Header.ImageButton" parent="Xyzu.View.Library.ImageButton">
    <item name="android:background">@drawable/xyzu_view_libraryheader_imagebutton_background</item>
