    <item name="android:textStyle">bold</item>
  </style>
  <style name="Xyzu.View.Info.Text.Key" parent="Xyzu.View.Info.Text">
    <item name="android:layout_marginStart">@dimen/dp32</item>
    <item name="android:maxWidth">@dimen/dp128</item>
