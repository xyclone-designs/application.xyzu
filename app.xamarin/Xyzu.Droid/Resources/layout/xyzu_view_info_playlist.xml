<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Title"
        android:id="@+id/xyzu_view_info_playlist_title_appcompattextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/xyzu_view_info_playlist_header"
            
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_info_playlist_title_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        app:barrierMargin="@dimen/dp32"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="xyzu_view_info_playlist_title_appcompattextview"/>  
    <!---->

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_info_playlist_barrier_rowoneend"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
                
        app:barrierDirection="end"
        app:constraint_referenced_ids="
        xyzu_view_info_playlist_playlistname_title_appcompattextview,
        xyzu_view_info_playlist_playlistdatecreated_title_appcompattextview,
        xyzu_view_info_playlist_playlistdatemodified_title_appcompattextview,
        xyzu_view_info_playlist_playlistduration_title_appcompattextview,
        xyzu_view_info_playlist_playlistsongcount_title_appcompattextview"/>

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Key"
        android:id="@+id/xyzu_view_info_playlist_playlistname_title_appcompattextview"
        android:text="@string/models_playlist_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_info_playlist_title_barrier_bottom"/>
                  
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Value"                                 
        android:id="@+id/xyzu_view_info_playlist_playlistname_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xyzu_view_info_playlist_barrier_rowoneend"
        app:layout_constraintTop_toTopOf="@id/xyzu_view_info_playlist_playlistname_title_appcompattextview"/>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_info_playlist_playlistname_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
        xyzu_view_info_playlist_playlistname_title_appcompattextview,
        xyzu_view_info_playlist_playlistname_value_appcompattextview"/>  
    <!---->

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Key"
        android:id="@+id/xyzu_view_info_playlist_playlistdatecreated_title_appcompattextview"
        android:text="@string/models_playlist_datecreated"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            
        app:layout_constrainedWidth="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_info_playlist_playlistname_barrier_bottom"/>
                  
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Value"                                 
        android:id="@+id/xyzu_view_info_playlist_playlistdatecreated_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xyzu_view_info_playlist_barrier_rowoneend"
        app:layout_constraintTop_toTopOf="@id/xyzu_view_info_playlist_playlistdatecreated_title_appcompattextview"/>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_info_playlist_playlistdatecreated_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
        xyzu_view_info_playlist_playlistdatecreated_title_appcompattextview,
        xyzu_view_info_playlist_playlistdatecreated_value_appcompattextview"/>  
    <!---->

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Key"
        android:id="@+id/xyzu_view_info_playlist_playlistdatemodified_title_appcompattextview"
        android:text="@string/models_playlist_datemodified"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            
        app:layout_constrainedWidth="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_info_playlist_playlistdatecreated_barrier_bottom"/>
                  
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Value"                                 
        android:id="@+id/xyzu_view_info_playlist_playlistdatemodified_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xyzu_view_info_playlist_barrier_rowoneend"
        app:layout_constraintTop_toTopOf="@id/xyzu_view_info_playlist_playlistdatemodified_title_appcompattextview"/>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_info_playlist_playlistdatemodified_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
        xyzu_view_info_playlist_playlistdatemodified_title_appcompattextview,
        xyzu_view_info_playlist_playlistdatemodified_value_appcompattextview"/> 
    <!---->

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Key"
        android:id="@+id/xyzu_view_info_playlist_playlistduration_title_appcompattextview"
        android:text="@string/models_playlist_duration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            
        app:layout_constrainedWidth="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_info_playlist_playlistdatemodified_barrier_bottom"/>
                  
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Value"                                 
        android:id="@+id/xyzu_view_info_playlist_playlistduration_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xyzu_view_info_playlist_barrier_rowoneend"
        app:layout_constraintTop_toTopOf="@id/xyzu_view_info_playlist_playlistduration_title_appcompattextview"/>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_info_playlist_playlistduration_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
        xyzu_view_info_playlist_playlistduration_title_appcompattextview,
        xyzu_view_info_playlist_playlistduration_value_appcompattextview"/>    
    <!---->

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Key"
        android:id="@+id/xyzu_view_info_playlist_playlistsongcount_title_appcompattextview"
        android:text="@string/models_playlist_songs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            
        app:layout_constrainedWidth="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_info_playlist_playlistduration_barrier_bottom"/>

    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.Info.Text.Value"                                 
        android:id="@+id/xyzu_view_info_playlist_playlistsongcount_value_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xyzu_view_info_playlist_barrier_rowoneend"
        app:layout_constraintTop_toTopOf="@id/xyzu_view_info_playlist_playlistsongcount_title_appcompattextview"/>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_info_playlist_playlistsongcount_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
        xyzu_view_info_playlist_playlistsongcount_title_appcompattextview,
        xyzu_view_info_playlist_playlistsongcount_value_appcompattextview"/>      
    <!---->

</merge>



