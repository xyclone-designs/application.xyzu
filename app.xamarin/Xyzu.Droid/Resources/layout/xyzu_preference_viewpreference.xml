<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/Xyzu.Preference"
    
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <include
        android:id="@+id/include_xyzu_preference_preference"
        layout="@layout/xyzu_preference_preference"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        
        app:layout_constraintWidth="match_constraint"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/xyzu_preference_viewpreference_contentframelayout"/>

    <androidx.appcompat.widget.ContentFrameLayout
        android:id="@+id/xyzu_preference_viewpreference_contentframelayout"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        
        app:layout_constraintWidth="match_parent"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/include_xyzu_preference_preference"/>

</androidx.constraintlayout.widget.ConstraintLayout>