<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.InfoEdit.Text.Title"
        android:id="@+id/xyzu_view_infoedit_genre_title_appcompattextview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/xyzu_view_infoedit_genre_header"
        
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_infoedit_genre_title_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        app:barrierMargin="@dimen/dp32"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="xyzu_view_infoedit_genre_title_appcompattextview"/>  
    <!---->

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.InfoEdit.Text.Key"
        android:id="@+id/xyzu_view_infoedit_genre_genrename_title_appcompattextview"
                    
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_genre_name"
                    
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_infoedit_genre_title_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatEditText
        style="@style/Xyzu.View.InfoEdit.EditText"
        android:id="@+id/xyzu_view_infoedit_genre_genrename_value_appcompatedittext"
                    
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
                    
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_infoedit_genre_genrename_title_appcompattextview"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_infoedit_genre_genrename_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierMargin="@dimen/dp16"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="xyzu_view_infoedit_genre_genrename_value_appcompatedittext"/>  
    <!---->

</merge>



