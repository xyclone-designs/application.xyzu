<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.InfoEdit.Text.Title"
        android:id="@+id/xyzu_view_infoedit_album_title_appcompattextview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/xyzu_view_infoedit_album_header"
        
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.appcompat.widget.AppCompatImageView
        style="@style/Xyzu.View.InfoEdit.Image"
        android:id="@+id/xyzu_view_infoedit_album_albumartwork_appcompatimageview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
            
        app:layout_constrainedWidth="true"
        app:layout_constrainedHeight="true"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_infoedit_album_title_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        
        app:barrierMargin="@dimen/dp0"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="xyzu_view_infoedit_album_title_appcompattextview,xyzu_view_infoedit_album_albumartwork_appcompatimageview"/>
    <!---->

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.InfoEdit.Text.Key"
        android:id="@+id/xyzu_view_infoedit_album_albumtitle_title_appcompattextview"
                    
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_album_title"
                    
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_infoedit_album_title_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatEditText
        style="@style/Xyzu.View.InfoEdit.EditText"
        android:id="@+id/xyzu_view_infoedit_album_albumtitle_value_appcompatedittext"
                    
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
                    
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_infoedit_album_albumtitle_title_appcompattextview"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_infoedit_album_albumtitle_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierMargin="@dimen/dp8"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="xyzu_view_infoedit_album_albumtitle_value_appcompatedittext"/>
    <!---->

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.InfoEdit.Text.Key"
        android:id="@+id/xyzu_view_infoedit_album_albumartist_title_appcompattextview"
                    
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_album_artist"
                    
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_infoedit_album_albumtitle_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatEditText
        style="@style/Xyzu.View.InfoEdit.EditText"
        android:id="@+id/xyzu_view_infoedit_album_albumartist_value_appcompatedittext"
                    
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
                    
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_infoedit_album_albumartist_title_appcompattextview"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_infoedit_album_albumartist_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierMargin="@dimen/dp8"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="xyzu_view_infoedit_album_albumartist_value_appcompatedittext"/>
    <!---->

    <!---->
    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.InfoEdit.Text.Key"
        android:id="@+id/xyzu_view_infoedit_album_albumreleasedate_title_appcompattextview"
                    
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/models_album_releasedate"
                    
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_infoedit_album_albumartist_barrier_bottom"/>
    <androidx.appcompat.widget.AppCompatEditDate
        style="@style/Xyzu.View.InfoEdit.EditDate"
        android:id="@+id/xyzu_view_infoedit_album_albumreleasedate_value_appcompateditdate"
                    
        android:layout_width="match_parent"
        android:layout_height="wrap_content"        
            
        app:dateDefaultText="@string/xyzu_view_infoedit_album_releasedate_defaulttext"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_infoedit_album_albumreleasedate_title_appcompattextview"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_infoedit_album_albumreleasedate_barrier_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierMargin="@dimen/dp16"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="xyzu_view_infoedit_album_albumreleasedate_value_appcompateditdate"/>
    <!---->

</merge>



