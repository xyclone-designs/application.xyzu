<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.AppCompatImageView
        style="@style/Xyzu.Preference.BasicText"
        android:id="@+id/xyzu_view_preference_license_viewmoreicon_appcompatimageview"
        android:layout_width="@dimen/dp32"
        android:layout_height="@dimen/dp32"
        android:layout_marginStart="@dimen/dp16"
        android:src="@drawable/icon_general_web"
        
        app:layout_constraintWidth="wrap_content"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintTop_toTopOf="@id/xyzu_view_preference_license_viewmore_appcompattextview"
        app:layout_constraintBottom_toBottomOf="@id/xyzu_view_preference_license_viewmore_appcompattextview"/>

    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.Preference.BasicText"
        android:id="@+id/xyzu_view_preference_license_viewmore_appcompattextview"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        android:layout_marginEnd="@dimen/dp16"
        android:layout_marginStart="@dimen/dp8"
        android:layout_marginVertical="@dimen/dp16"
        android:text="@string/extra_presstoviewmore"
        
        app:layout_constraintWidth="wrap_content"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintTop_toTopOf="parent" 
        app:layout_constraintBottom_toTopOf="@id/xyzu_view_preference_license_license_appcompattextview"
        app:layout_constraintStart_toEndOf="@id/xyzu_view_preference_license_viewmoreicon_appcompatimageview"/>

    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.Preference.SummaryText"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        android:layout_margin="@dimen/dp16"
        android:id="@+id/xyzu_view_preference_license_license_appcompattextview"
        
        app:layout_constraintWidth="match_parent"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_preference_license_viewmore_appcompattextview" />

</merge>