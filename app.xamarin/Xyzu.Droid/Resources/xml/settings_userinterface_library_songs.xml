<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

  <xyzu.preference.SwitchPreference
      app:iconSpaceReserved="false"
      app:key="settings_userinterface_library_songs_isreversed_switchpreference_key"
      app:title="@string/settings_userinterface_library_songs_isreversed_title"
      app:summary="@string/settings_userinterface_library_songs_isreversed_description"
      app:summaryOn="@string/settings_userinterface_library_songs_isreversed_description_on"
      app:summaryOff="@string/settings_userinterface_library_songs_isreversed_description_off"/>

  <xyzu.preference.ListPreference
      app:iconSpaceReserved="false"
      app:key="settings_userinterface_library_songs_layouttype_listpreference_key"
      app:title="@string/settings_userinterface_library_songs_layouttype_title"
      app:summary="@string/settings_userinterface_library_songs_layouttype_description"
