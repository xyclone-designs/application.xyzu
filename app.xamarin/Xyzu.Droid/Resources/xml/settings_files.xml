<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

  <xyzu.preference.DialogPreference
      app:iconSpaceReserved="false"
      app:key="settings_files_directories_dialogpreference_key"
      app:title="@string/settings_files_directories_title"
      app:summary="@string/settings_files_directories_description"
      app:dialogTitle="@string/settings_files_directories_dialog_title"
      app:negativeButtonText="@string/settings_files_directories_dialog_negativebutton"
      app:positiveButtonText="@string/settings_files_directories_dialog_positivebutton"/>

  <xyzu.preference.MultiSelectListPreference
      app:iconSpaceReserved="false"
      app:key="settings_files_mimetypes_multiselectlistpreference_key"
      app:title="@string/settings_files_mimetypes_title"
      app:summary="@string/settings_files_mimetypes_description"
      app:dialogTitle="@string/settings_files_mimetypes_dialog_title"
      app:negativeButtonText="@string/settings_files_mimetypes_dialog_negativebutton"
      app:positiveButtonText="@string/settings_files_mimetypes_dialog_positivebutton"/>

  <xyzu.preference.SeekBarPreference
      app:showSeekBarValue="true"
      app:iconSpaceReserved="false"
      app:key="settings_files_tracklengthignore_seekbarpreference_key"
      app:title="@string/settings_files_tracklengthignore_title"
      app:summary="@string/settings_files_tracklengthignore_description"/>

  <xyzu.preference.NavigationBarInsetPreference />

</androidx.preference.PreferenceScreen>