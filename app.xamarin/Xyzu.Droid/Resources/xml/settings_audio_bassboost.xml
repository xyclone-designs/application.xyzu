<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

  <xyzu.preference.SwitchPreference
      app:iconSpaceReserved="false"
      app:key="settings_audio_bassboost_isenabled_switchpreference_key"
      app:title="@string/settings_audio_bassboost_isenabled_title"
      app:summary="@string/settings_audio_bassboost_isenabled_description"
      app:summaryOn="@string/settings_audio_bassboost_isenabled_description_on"
      app:summaryOff="@string/settings_audio_bassboost_isenabled_description_off"/>

  <xyzu.preference.ListPreference
      app:iconSpaceReserved="false"
      app:key="settings_audio_bassboost_currentpreset_listpreference_key"
      app:title="@string/settings_audio_bassboost_currentpreset_title"
      app:summary="@string/settings_audio_bassboost_currentpreset_description"
      app:dialogTitle="@string/settings_audio_bassboost_currentpreset_dialog_title"
      app:negativeButtonText="@string/settings_audio_bassboost_currentpreset_dialog_negativebutton"
      app:positiveButtonText="@string/settings_audio_bassboost_currentpreset_dialog_positivebutton"/>

  <xyzu.preference.SeekBarPreference   
      app:adjustable="true"
      app:showSeekBarValue="true"
      app:iconSpaceReserved="false"
      app:updatesContinuously="true"
      app:key="settings_audio_bassboost_strength_seekbarpreference_key"
      app:title="@string/settings_audio_bassboost_strength_title"
      app:summary="@string/settings_audio_bassboost_strength_description"/>

  <xyzu.preference.NavigationBarInsetPreference />

</androidx.preference.PreferenceScreen>