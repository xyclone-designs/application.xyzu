<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
        
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Xyzu.Views.Insets.StatusBarInsetView
        android:id="@+id/xyzu_view_libraryitem_header_artist_statusbarinsetview"
        android:layout_height="@dimen/dp0"
        android:layout_width="@dimen/dp0"
        
        app:layout_constraintWidth="match_constraint"
        app:layout_constraintHeight="wrap_content_constrained"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" 
        app:layout_constraintBottom_toTopOf="@id/xyzu_view_libraryitem_header_artist_artwork_appcompatimageview" />

    <androidx.appcompat.widget.AppCompatImageView
        style="@style/Xyzu.View.LibraryItem.Header.Image"
        android:id="@+id/xyzu_view_libraryitem_header_artist_artwork_appcompatimageview"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp0"
        android:layout_marginTop="@dimen/dp32"    
        android:layout_marginStart="@dimen/dp16"
        android:scaleType="center"
        
        app:layout_constrainedWidth="true"
        app:layout_constrainedHeight="true"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintWidth_max="@dimen/dp630"
        app:layout_constraintWidth_min="@dimen/dp112"
        app:layout_constraintWidth_percent="0.3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_libraryitem_header_artist_statusbarinsetview"
        app:layout_constraintBottom_toTopOf="@id/xyzu_view_libraryitem_header_artist_artistitems_headerlibraryitemsview"/>

    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.LibraryItem.Header.Text.Title"
        android:id="@+id/xyzu_view_libraryitem_header_artist_lineone_appcompattextview"
        android:layout_height="@dimen/dp0"
        android:layout_width="@dimen/dp0"
        android:visibility="gone"
                
        app:layout_constraintWidth="wrap_content_constrained"
        app:layout_constraintHeight="wrap_content_constrained"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_chainStyle="packed"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/xyzu_view_libraryitem_header_artist_artwork_appcompatimageview"
        app:layout_constraintStart_toEndOf="@id/xyzu_view_libraryitem_header_artist_artwork_appcompatimageview"
        app:layout_constraintBottom_toTopOf="@id/xyzu_view_libraryitem_header_artist_linetwo_appcompattextview" />

    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.LibraryItem.Header.Text"
        android:id="@+id/xyzu_view_libraryitem_header_artist_linetwo_appcompattextview"
        android:layout_height="@dimen/dp0"
        android:layout_width="@dimen/dp0"
        android:visibility="gone"
                
        app:layout_constraintWidth="wrap_content_constrained"
        app:layout_constraintHeight="wrap_content_constrained"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xyzu_view_libraryitem_header_artist_artwork_appcompatimageview"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_libraryitem_header_artist_lineone_appcompattextview"
        app:layout_constraintBottom_toTopOf="@id/xyzu_view_libraryitem_header_artist_linethree_appcompattextview"/>

    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.LibraryItem.Header.Text"
        android:id="@+id/xyzu_view_libraryitem_header_artist_linethree_appcompattextview"
        android:layout_height="@dimen/dp0"
        android:layout_width="@dimen/dp0"
        android:visibility="gone"
                
        app:layout_constraintWidth="wrap_content_constrained"
        app:layout_constraintHeight="wrap_content_constrained"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xyzu_view_libraryitem_header_artist_artwork_appcompatimageview"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_libraryitem_header_artist_linetwo_appcompattextview"
        app:layout_constraintBottom_toTopOf="@id/xyzu_view_libraryitem_header_artist_linefour_appcompattextview"/>

    <androidx.appcompat.widget.AppCompatTextView
        style="@style/Xyzu.View.LibraryItem.Header.Text"
        android:id="@+id/xyzu_view_libraryitem_header_artist_linefour_appcompattextview"
        android:layout_height="@dimen/dp0"
        android:layout_width="@dimen/dp0"
        android:visibility="gone"
                
        app:layout_constraintWidth="wrap_content_constrained"
        app:layout_constraintHeight="wrap_content_constrained"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/xyzu_view_libraryitem_header_artist_artwork_appcompatimageview"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_libraryitem_header_artist_linethree_appcompattextview"
        app:layout_constraintBottom_toTopOf="@id/xyzu_view_libraryitem_header_artist_artistitems_headerlibraryitemsview"/>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/xyzu_view_libraryitem_header_artist_artistsongs_barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
                
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="
        xyzu_view_libraryitem_header_artist_artwork_appcompatimageview,
        xyzu_view_libraryitem_header_artist_linefour_appcompattextview"/>

    <xyzu.views.libraryitem.header.HeaderLibraryItemsView
        style="@style/Xyzu.View.LibraryItem.Header.LibraryItems"
        android:id="@+id/xyzu_view_libraryitem_header_artist_artistitems_headerlibraryitemsview"
        android:layout_height="@dimen/dp0"
        android:layout_width="@dimen/dp0"       
        android:layout_marginTop="@dimen/dp32"             
        
        app:layout_constraintWidth="match_constraint"
        app:layout_constraintHeight="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/xyzu_view_libraryitem_header_artist_artistsongs_barrier"/>

</merge>
